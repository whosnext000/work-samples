    function buildURL() {
        var url = "/aircraft-for-sale/";
        if ( $("#so-search").html() != "Select" && $("#so-search").html() != "") {
            var temp = $("#so-search").html();
            var temp2 = formatModelDes(temp);
            url += temp2;
        }
        else if ($("#sm-search").html() != "Select" && $("#sm-search").html() != "") {
            var temp = $("#sc-search").html();
            var tempA = formatModelDes(temp); //temp.replace(/\s/g, '-');
            var temp2 = $("#sm-search").html();
            var tempB = formatModelDes(temp2);// temp2.replace(/\s/g, '-');
            url += tempA + "/" + tempB;
        }
        else if ($("#sc-search").html() != "Select") {
            var temp = $("#sc-search").html();
            var tempA = formatModelDes(temp, ''); // temp.replace(/\s/g, '-');
            url += tempA;
        }
        else if ($("#sc-search").html() == "Select") {
            url = "no";
        }
        else {
            url = "no";
        }
        return url;
    }

