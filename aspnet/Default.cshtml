    function startSlideShowAnimation2(x) {
        var go;
        go = setTimeout(startout, 5500);
        function startout() {
            $('#slide' + currentSlide).fadeOut(2000);
            swap();
        }
        function swap() {
            if ((currentSlide + 1) < maxSlidesInSlideShow) {
                currentSlide++;
                $('#slide' + currentSlide).fadeIn(3000);
            }
            else {
                currentSlide = 0;
                $('#slide' + currentSlide).fadeIn(3000);
            }
            setTimeout(startout, 9000);
        }
    }

