@section scripts {

<script type="text/javascript">
    var vtoken = '';
    var last, imgcnt = null;
    var imgtimer = null;
    var currentSlide = 0;
    var loaded = 0;
    var maxSlidesInSlideShow = 4;
    var ismobile = @ViewBag.IsMobile || 0;
    $(function () {
        setafsDefMod();
        processtblMetaInput(afsDefMod);
        getCompanies();
        getSearchLocations();
        /* end build accordion show*/
        var c = readCookie("clsInterest");
        c = parseInt((c == undefined) ? 5 : c);
        if (isNaN(c)) {
            c = 5;
        }
        select_classification(c);

        for (j = 1; j <= 5; j++) {
            var ele = 'sq-ad-' + j;
            insertBanManAd(ele, 'squareZones', j);
        }
        
        //if ((ismobile == 1) || (!Modernizr.mq('(min-width: 651px)'))) {
        //    $("#bottomdiv").html($("#topdiv1").html());
        //    $("#topdiv1").empty();
        //    $("#topdiv1").hide();
        //    $(".hide-when-mobile").hide();
        //}

        renderJustPosted(afsDefMod.JustPostedLst);

        $("#search-button").click(function (e) {
            e.preventDefault();
            btnSrchCMMD_Click();
        });
        $("#clear-button").click(function (e) {
            e.preventDefault();
            btnSrchClear_Click();
        });
        $("#chkUSA").change(function (e) {
            setUSASelection(e);
        })
        $("#sc-search").css('height', 'auto');
        $('#quicksearch').keyup(function (e) {
            var length = $('#quicksearch').val().length;
            if (e.keyCode == 13) {
                if (length > 2) {
                    $("#frmpostmethod").prop("action");
                    $("#frmpostmethod").submit();
                }
                else {
                    alert('Quicksearch requires at least 3 characters.');
                }
            }

        })
        $(".ts-circle").click(function (e) {
            var length = $('#quicksearch').val().length;
            e.preventDefault();
            if (length > 2) {
                $("#frmpostmethod").prop("action");
                $("#frmpostmethod").submit();
            }
            else {
                alert('Quicksearch requires at least 3 characters.');
            }
        })
        setTimeout(function () {
            var count = 1;
            $(".acimg").each(function () {
                var h = this.height;
                if (h > 0) {
                    var n = (115 - h) / 2;
                    var x = parseInt(n);
                    $('.ac' + count).css('padding-top', x + 'px');
                }
                count++;
            });
        }, 100);
        setTimeout(function () {
            var count = 1;
            $(".acimg").each(function () {
                var h = this.height;
                if (h > 0) {
                    var n = (115 - h) / 2;
                    var x = parseInt(n);
                    $('.ac' + count).css('padding-top', x + 'px');
                }
                count++;
            });
        }, 1500);
        setTimeout('getAdsWithMostInterest("' + afsDefMod.MVStartCls + '")', 100);
        tempdata = '';
        currentSlide = Math.floor((Math.random() * 4));
        setTimeout(function () {
            startSlideShowAnimation2(currentSlide);
        }, 500);
    });

    $(window).resize(function () {
        if (last != undefined) {
            $("#cl" + last + "-content").css({ "display": "none" });
            $("#cl" + last + "-content").empty();
            $(".slide-details").empty();

            var current = last;
            if (current != 1) {
                last = 1;
            }
            else {
                last = 2;
            }
            select_classification(current);
        }
    });

    function select_classification(id) {
        buildSlideShow2(id);

        if ($(window).width() > 843) {
            if ($(window).width() > 1570) {
                if (last != undefined)
                    $("#cl" + last + "-content").animate({ width: "0px" }, "slow");
                $("#cl" + id + "-content").css("display", "block").animate({ width: "550px" }, "slow");
                $("#b-cl" + id).css("display", "none");
            }
            else {
                if (last != undefined)
                    $("#cl" + last + "-content").animate({ width: "0px" }, "slow");
                $("#cl" + id + "-content").css("display", "block").animate({ width: "350px" }, "slow");
                $("#b-cl" + id).css("display", "none");
            }
        }
        else {
            if ($(window).width() > 650) {
